{
    "env": {
        "es2020": true,
        "node": true
    },
    "extends": [
        "airbnb-base",
        "airbnb-typescript/base"
        
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module",
        "project":"./tsconfig.json"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "no-unused-vars": "off",
        "@typescript-eslint/no-unused-vars": "error"
    },
    "settings": {
        "import/extensions": [".js",".ts"],
        "import/parsers": {
          "@typescript-eslint/parser": [".ts"]
         },
        "import/resolver": {
        "node": {
            "extensions": [".js",".ts"]
            }
        }
    }
}
